<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.bank.domain.repository.BankMoneyRepository">
    <select id="selectListBankMoney" resultType="org.bank.domain.entity.BankMoney">
     select * from bank_money
      where username=#{username}
  </select>
</mapper>